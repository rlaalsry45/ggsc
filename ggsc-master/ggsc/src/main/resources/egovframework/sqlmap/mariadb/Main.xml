<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="AdmMain">

	<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>

	<select id="mainDao.getMainExtReport" parameterClass="egovMap" resultClass="egovMap">
		<!-- 외부활동 차트 -->
		select user_id from user_m where user_id = 'admin'
	</select>

	<select id="mainDao.getMainYcReport" parameterClass="egovMap" resultClass="egovMap">
		<!--연차 사용 차트 -->
		select user_id from user_m where user_id = 'admin'
	</select>

	<select id="mainDao.getMainOvWeekReport" parameterClass="egovMap" resultClass="egovMap">
		<!-- 초과 주 차트 -->
		select user_id from user_m where user_id = 'admin'
	</select>

	<select id="mainDao.getMainOvMonthReport" parameterClass="egovMap" resultClass="egovMap">
		<!-- 초과 월 차트 -->
		select user_id from user_m where user_id = 'admin'
	</select>

</sqlMap>
